{
  "scripts": {
    "eslint": "eslint --fix app/",
    "stylus": "stylus --sourcemap app/stylesheets/styl/app.styl --out ../resources/static/",
    "bundle": "webpack",
    "copy:angular": "cp node_modules/angular/angular.min.js ../resources/static/",
    "copy:angular-map": "cp node_modules/angular/angular.min.js.map ../resources/static/",
    "test": "npm-run-all eslint stylus bundle* --parallel copy:*"
  },
  "devDependencies": {
    "eslint": "^4.13.1",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^3.1.1",
    "lodash": "4.17.11",
    "node-libs-browser": "0.5.3",
    "npm-run-all": "^4.1.2",
    "stylus": "0.54.5",
    "webpack": "3.8.1"
  },
  "license": "GPL-3.0+",
  "dependencies": {
    "angular": "1.7.9"
  }
}
